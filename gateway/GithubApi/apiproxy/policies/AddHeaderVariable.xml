<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AddHeaderVariable">
    <DisplayName>AddHeaderVariable</DisplayName>
    <FaultRules/>
    <Properties/>
   
    <Add>
        <Headers>
          <Header name = "myEmail">{userName}</Header>
      </Headers>
    </Add>
    
   
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
